{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"]
    }
  ],
  "env": {
    "VITE_API_URL": "https://api-scribes-quill-90be4514fafb.herokuapp.com"
  }
}